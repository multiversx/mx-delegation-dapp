    "build-testnet": "yarn run copy-testnet-config & yarn build",
    "build-mainnet": "yarn run copy-mainnet-config & yarn build",
    "test": "node scripts/test.js",
    "copy-testnet-config": "cp ./src/config.testnet.ts ./src/config.ts",
    "copy-mainnet-config": "cp ./src/config.mainnet.ts ./src/config.ts",
    "lint": "eslint --ext js,ts,tsx src --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
